<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
	   http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	   http://code.alibabatech.com/schema/dubbo
	   http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <!-- 提供方应用信息，用于计算依赖关系 -->
    <dubbo:application name="rrcp-service" owner="hpw_service"/>

    <!-- 使用zookeeper注册中心暴露服务地址 -->
    <dubbo:registry protocol="zookeeper" address="${dubbo.registry.address}"/>

    <!-- 用dubbo协议在20880端口暴露服务 -->
    <!--<dubbo:protocol name="dubbo" port="${dubbo.provider.user.protocol.port}"/>-->
    <!--<dubbo:protocol name="dubbo" port="${dubbo.provider.user.protocol.port1}"/>-->
    <!--<dubbo:protocol name="dubbo" port="${dubbo.provider.user.protocol.port2}"/>-->
    <!--<dubbo:protocol name="dubbo" port="${dubbo.provider.user.protocol.port3}"/>-->
    <dubbo:protocol name="dubbo" port="${dubbo.provider.user.protocol.port4}"/>
    <!--<dubbo:protocol name="dubbo" port="${dubbo.provider.user.protocol.port5}"/>-->
    <!--<dubbo:protocol name="dubbo" port="${dubbo.provider.user.protocol.port6}"/>-->
    <!--<dubbo:protocol name="dubbo" port="${dubbo.provider.user.protocol.port7}"/>-->
    <!--<dubbo:protocol name="dubbo" port="${dubbo.provider.user.protocol.port8}"/>-->
    <!--<dubbo:protocol name="dubbo" port="${dubbo.provider.user.protocol.port9}"/>-->
    <!--<dubbo:protocol name="dubbo" port="${dubbo.provider.user.protocol.port10}"/>-->
    <!--<dubbo:protocol name="dubbo" port="${dubbo.provider.user.protocol.port11}"/>-->

    <!-- 当ProtocolConfig和ServiceConfig某属性没有配置时,采用此缺省值 -->
    <dubbo:provider timeout="10000" threadpool="fixed" threads="2048" accepts="2048"/>

    <!-- 提供的服务接口 -->
    <dubbo:service retries="2" interface="com.rrcp.api.user.service.impl.UService" ref="uService"/>

</beans>